<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="view_pmanagement_tool_form" model="ir.ui.view">
			<field name="name">view.pmanagement.tool.form</field>
			<field name="model">project.management.hr</field>
			<field name="arch" type="xml">
				<form >
					<sheet>
						<group>
							<field name="name" />
						</group>
					</sheet>
				</form>
			</field>
		</record>
		<record id="view_pmanagement_tool_list" model="ir.ui.view">
			<field name="name">view.pmanagement.tool.list</field>
			<field name="model">project.management.hr</field>
			<!-- <field eval="8" name="priority"/> -->
			<field name="arch" type="xml">
				<tree  editable="bottom">
					<field name="name" />
				</tree>
			</field>
		</record>
		<record id="open_project_form" model="ir.actions.act_window">
			<field name="name">Project Management</field>
			<field name="res_model">project.management.hr</field>
			<field name="view_mode">tree,form</field>
		</record>

        <menuitem
                    id="menu_config_pops_types"
                    name="Project Management"
                    action="open_project_form"
                    parent="hr.menu_config_plan"
                    groups="base.group_no_one"
                    sequence="99"/>
	</data>
</odoo>
