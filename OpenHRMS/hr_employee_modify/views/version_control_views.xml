<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="view_version_control_form" model="ir.ui.view">
			<field name="name">version.control.form</field>
			<field name="model">version.control.hr</field>
			<field name="arch" type="xml">
				<form string="Employee version Control">
					<sheet>
						<group>
							<field name="name" />
						</group>
					</sheet>
				</form>
			</field>
		</record>
		<record id="view_version_control_list" model="ir.ui.view">
			<field name="name">version.control.list</field>
			<field name="model">version.control.hr</field>
			<!-- <field eval="8" name="priority"/> -->
			<field name="arch" type="xml">
				<tree string="Employees version Control" editable="bottom">
					<field name="name" />
				</tree>
			</field>
		</record>
		<record id="open_version_control_form" model="ir.actions.act_window">
			<field name="name">Employee version Control</field>
			<field name="res_model">version.control.hr</field>
			<field name="view_mode">tree,form</field>
		</record>

        <menuitem
                    id="menu_config_version_control_types"
                    name="version Control"
                    action="open_version_control_form"
                    parent="hr.menu_config_plan"
                    groups="base.group_no_one"
                    sequence="99"/>
	</data>
</odoo>
